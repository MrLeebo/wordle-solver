{
  "private": true,
  "name": "wordle-solver",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./app/styles/global.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./app/styles/global.css -o app/styles/app.css",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/react": "^0.21.0",
    "@remix-run/serve": "^0.21.0",
    "@types/react": "17.0.39",
    "concurrently": "7.0.0",
    "flowbite": "1.3.3",
    "postcss": "8.4.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "remix": "^0.21.0",
    "tailwindcss": "3.0.20",
    "tiny-invariant": "1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.21.0"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "keywords": []
}
